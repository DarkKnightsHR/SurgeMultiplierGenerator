const sqs = require('../sqs');
const mocha = require('mocha');

describe('sqs', () => {
  describe('sqs receive eyeballs', () => {
    it('should receive messages without errors', (done) => {
      sqs.receiveEyeballs();
    });
  });

  describe('sqs receive bookings', () => {
    it('should receive messages without errors', (done) => {
      sqs.receiveBookings();
    });
  });

  describe('sqs receive drivers', () => {
    it('should receive messages without errors', (done) => {
      sqs.receiveDrivers();
    });
  });

  describe('sqs send multipliers', () => {
    it('should send messages without errors', (done) => {
      sqs.sendMultipliers();
    });
  });

});
